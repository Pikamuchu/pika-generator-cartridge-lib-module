{
  "name": "<%= moduleName %>",
  "version": "0.0.1",
  "description": "<%- description %>",
  "main": "dist/<%= fileName %>",
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "build": "babel src -d dist/server --ignore \"src/client/**/*\"",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "sgmf-scripts --lint js",
    "prettier": "prettier --write \"./lib/*.js\"",
    "test": "nyc mocha --require @babel/register",
    "upload": "sgmf-scripts --upload -- ",
    "uploadCartridge": "sgmf-scripts --uploadCartridge <%- lib_module %>",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= user %>/<%= repo %>.git"
  },
  "keywords": [],
  "author": "<%- author %>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/<%= user %>/<%= repo %>/issues"
  },
  "homepage": "https://github.com/<%= user %>/<%= repo %>",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-subset": "^1.5.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-mocha": "^5.3.0",
    "istanbul": "^0.4.4",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^1.13.0",
    "sinon": "^7.1.1",
    "sgmf-scripts": "^2.1.1",
    "sonar-scanner": "^3.1.0",
    "searchquire": "^1.4.0",
    "simple4x": "^1.2.1"
  },
  "babelBoilerplateOptions": {
    "entryFileName": "<%= fileName %>",
    "mainVarName": "<%= lib_module %>"
  }
}
